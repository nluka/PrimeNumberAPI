{
  "name": "prime-number-api",
  "version": "1.0.0",
  "description": "A simple web API for prime numbers.",
  "main": "src/server.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest --verbose",
    "test-watch": "jest --watchAll",
    "dev": "nodemon src/server.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint -c .eslintrc.json",
    "build": "tsc",
    "start": "node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nluka/prime-number-api.git"
  },
  "keywords": [
    "prime",
    "number",
    "numbers",
    "api"
  ],
  "author": "Nicholas Lukasevich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nluka/prime-number-api/issues"
  },
  "homepage": "https://github.com/nluka/prime-number-api#readme",
  "devDependencies": {
    "@jest/globals": "^26.6.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/supertest": "^2.0.11",
    "eslint": "^7.25.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1"
  }
}
